type Partner {
  id: String!
  code: String
  type: String!
  active: Boolean!
  name: String!
  vat_number: String
  vat: Boolean!
  registration_number: String
  address: Address
}

input PartnerInput {
  id: String
  code: String
  name: String!
  type: String!
  vat_number: String
  vat:Boolean!
  registration_number: String
  address: AddressInput
  is_active: Boolean
}

extend type Query {
  getPartners: [Partner]
}

extend type Mutation {
  savePartner(input: PartnerInput!): String
}
